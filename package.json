{
  "name": "nextal",
  "private": false,
  "description": "Starter template for Next with TypeScript. Supports Tailwind with CSS-Modules. Jest and @react/testing-library configured and ready to go. Also ESLint, Prettier, Husky, Commit-lint and Atomic Design for components.",
  "homepage": "https://github.com/jvidalv/nextal#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvidalv/nextal.git"
  },
  "author": "Josep Vidal <josepvidalvidal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jvidalv/nextal/issues"
  },
  "keywords": [
    "react",
    "next",
    "nextjs",
    "typescript",
    "jest",
    "tailwind",
    "template",
    "starter",
    "css-modules",
    "eslint",
    "prettier",
    "husky",
    "commit-lint"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not IE 11"
  ],
  "engines": {
    "node": ">=14",
    "yarn": ">=1.22.5"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "postinstall": "husky install",
    "ts": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "commit": "cz",
    "format": "npx prettier --check --ignore-path .gitignore .",
    "format:fix": "npx prettier --write --ignore-path .gitignore ."
  },
